{% extends 'persondegree/content.html.twig' %}

{% block referencement_h1 %}
  {{ parent() }} {{ 'referencement.degree_profile' | trans }}
{% endblock %}

{% block body %}

  <div class="content-i">
    <div class="content-box">
      {# Tableau 1 #}
      <div class="element-wrapper">
        <img src="{{ asset('build/images/icon/degree.png') }}" alt="locality" class="title-icon" style="margin-bottom: -35px;">
        <h6 class="element-header">
          {{ 'graduate.new_graduate' | trans }}
          {% if is_granted('ROLE_ADMIN') %}
          <a class="btn btn-sm btn-primary float-right" href="{{ path('persondegree_index') }}">
            <i class="fa fa-arrow-left"></i> {{ 'menu.back_to_the_list' | trans }}
          </a>
          {% endif %}
        </h6>
        <div class="element-box">
          <form id="form_person_degree" method="post" action="{{ path('persondegree_new' | route_persondegree) }}">
            {% include 'persondegree/_new.html.twig' %}
          </form>
        </div>
      </div>
    </div>
  </div>
{% endblock %}




