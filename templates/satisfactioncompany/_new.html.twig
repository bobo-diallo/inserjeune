      </div>
    </div>
    <div class="row">
      <div class="col-sm-12 form-group hide_flex_otherWorkerJob">
        <label for="">{{ 'survey.other_areas_of_specialization_for_your_workers' | trans }} ?</label>
        {{ form_widget(form.otherWorkerJob) }}
        <div class="help-block form-text with-errors form-control-feedback">{{ form_errors(form.otherWorkerJob) }}</div>
      </div>
    </div>
  </div>
  <div class="col-sm-6 form-group">
    <label for="">{{ 'survey.what_are_the_occupations_areas_of_specialization_of_your_workers_several_possible_choices' | trans }}</label>
    {{ form_widget(form.workerActivities) }}
    <div class="help-block form-text with-errors form-control-feedback">{{ form_errors(form.workerActivities) }}</div>
  </div>
</div>
<div class="line-separator"></div>
<div class="row">
  <div class="col-sm-6">
    <div class="row">
      <div class="col-sm-12 form-group">
        <label for="">{{ 'survey.what_is_the_sector_of_activity_of_your_technicians' | trans }} ?</label>
