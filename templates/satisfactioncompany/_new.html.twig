{{ form_widget(form._token) }}
<div class="form-group row">
  <div class="col-sm-5">
    {% if satisfactionCompany.company|length > 0 %}
       {{ 'menu.company' | trans }} :   {{ satisfactionCompany.company}}
    {% endif %}
  </div>
  {%  if app.user.roles|role =="ADMIN" %}
    <div class="col-sm-2">
      Id : {{ satisfactionCompany.id }}
    </div>
  {% endif %}
  <div class="col-sm-5">
    {{ 'menu.created_date' | trans }} : {{ satisfactionCompany.createdDate|date("d/m/Y H:i") }}
  </div>
</div>
<div class="line-separator"></div>
<div class="row">
  <div class="col-sm-4 form-group">
    <label for=""> {{ 'survey.how_many_employees_do_you_have' | trans }} ?</label>
    {{ form_widget(form.salaryNumber) }}
    <div class="help-block form-text with-errors form-control-feedback">{{ form_errors(form.salaryNumber) }}</div>
  </div>
  <div class="col-sm-4 form-group">
    <label for=""> {{ 'survey.how_many_apprentices_do_you_employ' | trans }} ?</label>
    {{ form_widget(form.apprenticeNumber) }}
    <div class="help-block form-text with-errors form-control-feedback">{{ form_errors(form.apprenticeNumber) }}</div>
  </div>
  <div class="col-sm-4 form-group">
    <label for=""> {{ 'survey.how_many_interns_do_you_employ' | trans }}  ?</label>
    {{ form_widget(form.studentNumber) }}
    <div class="help-block form-text with-errors form-control-feedback">{{ form_errors(form.studentNumber) }}</div>
  </div>
</div>
<div class="line-separator"></div>
<div class="row">
  <div class="col-sm-6">
    <div class="row">
      <div class="col-sm-12 form-group">
        <label for="">{{ 'survey.what_is_the_sector_of_activity_of_your_workers' | trans }} ?</label>
        {{ form_widget(form.workerSectorArea) }}
