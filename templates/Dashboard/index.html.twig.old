          <div class="element-wrapper"><h6 class="element-header">Entreprises par secteurs d'activités</h6>
            <div class="element-box element-box-reduct">
              <div class="el-chart-w">
                {#{% set companyActivities='["commerce", "artisanat", "tourisme", "santé", "enseignement", "batiment", "energie", "transport" ]'%}#}
                {#{% for company in companies %}#}
                {#<script>alert({{ company.name }})</script>#}
                {#{% endfor %}#}
                {% set companyActivities='["commerce", "artisanat", "tourisme", "santé", "enseignement", "batiment", "energie", "transport" ]'%}
                <canvas height="120" id="donutActivity" width="120" data-value="{{ companyActivities }}"></canvas>
                <div class="inside-donut-chart-label">
                  <strong>142</strong><span>Total Entreprises</span></div>

              </div>
              <div class="row legend_activity">
                <div class="col-sm-6 el-legend">
                  <div class="legend-value-w">
                    <div class="legend-pin" style="background-color: #85C441;"></div>
                    <div class="legend-value">commerce</div>
                  </div>
                  <div class="legend-value-w">
                    <div class="legend-pin" style="background-color: #FFD403;"></div>
                    <div class="legend-value">artisanat</div>
                  </div>
                  <div class="legend-value-w">
                    <div class="legend-pin" style="background-color: #74AAFF;"></div>
                    <div class="legend-value">tourisme</div>
                  </div>
                  <div class="legend-value-w">
                    <div class="legend-pin" style="background-color: #EE3233;"></div>
                    <div class="legend-value">santé</div>
                  </div>
                </div>
                <div class="col-sm-6 el-legend">
                  <div class="legend-value-w">
                    <div class="legend-pin" style="background-color: #8E1682;"></div>
                    <div class="legend-value">enseignement</div>
                  </div>
                  <div class="legend-value-w">
                    <div class="legend-pin" style="background-color: #FF8B00;"></div>
                    <div class="legend-value">batiment</div>
                  </div>
                  <div class="legend-value-w">
                    <div class="legend-pin" style="background-color: #1C1817;"></div>
                    <div class="legend-value">energie</div>
                  </div>
                  <div class="legend-value-w">
                    <div class="legend-pin" style="background-color: #BFBFBF;"></div>
                    <div class="legend-value">transport</div>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>


