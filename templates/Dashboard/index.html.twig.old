{% extends "base.html.twig" %}

{% block title %}
  {{ parent() }} Dashboard
{% endblock %}

{% block navigation %}
  <li class="breadcrumb-item"><span>Tableau de bord</span></li>
{% endblock %}

{% block body %}
  <div class="content-i">
    <div class="content-box">

      {#Taux de satisfactions en chiffre#}
      {#/******************************/#}
      <div class="row">
        <div class="col-sm-12">
          <div class="element-wrapper">
            <div class="element-actions">
                <form class="form-inline justify-content-sm-end">
                  <select class="form-control form-control-sm rounded">
                    <option value="Pending">Aujourd'hui</option>
                    <option value="Active">Dernière semaine</option>
                    <option value="Cancelled">Derniers 30 Jours</option>
                  </select>
                </form>
            </div>
            <h6 class="element-header">Taux d'insertion des diplômés et de satisfaction des Entreprises</h6>
            <div class="element-content satisfaction_value">
              <div class="row">
                <div class="col-sm-3">
                  <div class="element-box el-tablo">
                    <div class="label">Diplômés sans emploi</div>
                    <div class="value">20%</div>
                    <div class="trending trending-up"><span>12%</span><i
                          class="os-icon os-icon-arrow-up2"></i></div>
                  </div>
                </div>
                <div class="col-sm-3">
                  <div class="element-box el-tablo">
                    <div class="label">Diplômés salariés</div>
                    <div class="value">40%</div>
                    <div class="trending trending-down-basic"><span>12%</span><i
                          class="os-icon os-icon-arrow-2-down"></i></div>
                  </div>
                </div>
                <div class="col-sm-3">
                  <div class="element-box el-tablo">
                    <div class="label">Diplômés créateurs</div>
                    <div class="value">89%</div>
                    <div class="trending trending-down-basic"><span>9%</span><i
                          class="os-icon os-icon-graph-down"></i></div>
                  </div>
                </div>
                <div class="col-sm-3">
                  <div class="element-box el-tablo">
                    <div class="label">Entreprises</div>
                    <div class="value">25%</div>
                    <div class="trending trending-down-basic"><span>9%</span><i
                          class="os-icon os-icon-graph-down"></i></div>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>


      <div class="row">
        {#diagramme baton des satisfactions#}
        {#/*******************************/#}
        <div class="col-sm-6">
          <div class="element-wrapper">
            <h6 class="element-header">Taux d'insertion et de satisfaction</h6>
            <div class="element-box element-box-reduct">
              {#<h5 class="form-header">Powered by Chart.js</h5>#}
              {#<div class="form-desc">Description#}
              {#<a href="http://www.chartjs.org/" target="_blank">Lien vers satisfaction</a></div>#}
              <div class="el-chart-w graph_satisfactions">
                <canvas height="100px" id="barSatisfactions" width="80px"></canvas>
                <img class="legend_bar_satisfactions" src="{{ asset('build/images/legend_bar_satisfactions.jpg') }}">
              </div>
            </div>
          </div>
        </div>

        {#diagramme Anneau des secteurs d'activités#}
        {#/***************************************/#}
